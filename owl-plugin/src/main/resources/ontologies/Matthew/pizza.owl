<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.pizza.com/ontologies/pizza.owl#"
     xml:base="http://www.pizza.com/ontologies/pizza.owl"
     xmlns:pizza="http://www.pizza.com/ontologies/pizza.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.pizza.com/ontologies/pizza.owl">
        <rdfs:comment>A pizza ontology that describes various pizzas based on their toppings</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasAngle -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasAngle"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasPizzaSize -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasPizzaSize"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasSide -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasSide"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasDiameter -->

    <owl:DatatypeProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasDiameter">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#AmericanHotPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#AmericanHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#AmericanaPizza"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MargheritaPizza"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SohoPizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#AmericanaPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MargheritaPizza"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SohoPizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#AnchovyTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SeafoodTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PrawnTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TunaTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CaperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetableTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MushroomTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#OliveTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#OnionTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CheeseTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MeatTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SeafoodTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CheesyPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CheesyPizza">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:comment>A Cheesy Pizza must have one topping from the CheeseTopping</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Country -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Country">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#DeepPanBase -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#FourCheesePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#FourClassPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#FourClassPizza">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#InterestingPizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RedPepperTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#HamTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MeatTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SalamiTopping"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpcyBeefTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Hot -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Hot">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Medium"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#InterestingPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#InterestingPizza">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minCardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#RedPepperTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#LargePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#LargePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasPizzaSize"/>
                        <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#LargePizzaSize"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#LargePizzaSize -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#LargePizzaSize">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#PizzaSize"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasDiameter"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:oneOf>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&gt;=12</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </owl:oneOf>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaSize"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#MargheritaPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SohoPizza"/>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato toppings</rdfs:comment>
